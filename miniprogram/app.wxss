@import "dist/index.wxss";
page {
  height: 100%;
  overflow-x: hidden;
  background: #eee;
}

page, view, scroll-view {
  font-size: 32rpx;
  font-weight: 300;
  -webkit-overflow-scrolling: touch;
  box-sizing: border-box;
}

.page-no-data {
  color: #999;
  height: 300rpx;
  display: -webkit-box;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex-direction: column;
  -webkit-box-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  align-items: center;
}

.page-no-data .icon-cnode {
  font-size: 36px;
}

.page-no-data .text-desc {
  padding: 10rpx;
  font-size: 28rpx;
}

.page-float-tip {
  position: fixed;
  right: 80rpx;
  bottom: 120rpx;
  height: 80rpx;
  width: 80rpx;
  border-radius: 50%;
  overflow: hidden;
  background: rgba(255, 255, 255, 0.6);
  display: -webkit-box;
  display: flex;
  -webkit-box-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  align-items: center;
  box-sizing: border-box;
  border: 1px solid #ddd;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
  opacity: 0.9;
  box-shadow: 0 0 10rpx #eee;
}

.page-float-tip.w {
  height: 100rpx;
  width: 100rpx;
  display: block;
  overflow: hidden;
}

.page-float-tip.w .icon-box {
  display: block;
  height: 100rpx;
  width: 100rpx;
  display: -webkit-box;
  display: flex;
  -webkit-box-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  align-items: center;
  box-sizing: border-box;
  -webkit-transition: all 0.6s;
  transition: all 0.6s;
}

.page-float-tip.w .icon-box .icon-cnode {
  font-size: 46rpx;
}

.page-float-tip.w .icon-box.c {
  margin-top: 0;
}

.page-float-tip.w .icon-box.s {
  margin-top: -100rpx;
}

.page-float-tip .top-img {
  display: block;
  height: 50rpx;
  width: 50rpx;
}

.page-float-tip .icon-cnode {
  position: relative;
  top: -2rpx;
  font-size: 40rpx;
  color: #666;
  font-weight: 700;
}

@font-face {
  font-family: "icon-cnode";
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
}

.icon-cnode {
  font-family: "icon-cnode" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

